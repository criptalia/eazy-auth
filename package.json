{
  "name": "eazy-auth",
  "version": "0.3.0",
  "description": "Easy auth stuff with redux",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "example": "webpack-dev-server",
    "clean": "rimraf lib",
    "build": "babel src --out-dir lib",
    "watch": "babel --watch src --out-dir lib",
    "build:example": "rimraf build && webpack --config webpack.prod.js",
    "prepublishOnly": "npm run clean && npm run build",
    "deploy": "npm run build:example && cpr 404.html build/ && gh-pages -d build"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.9",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^2.30.1",
    "lodash": "^4.17.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.1",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "hoist-non-react-statics": "^2.3.1"
  },
  "peerDependencies": {
    "react": "^15.0.0-0 || ^16.0.0-0",
    "react-redux": "^4.3.0 || ^5.0.0",
    "redux": "^3.0.0",
    "redux-saga": ">=0.15.0",
    "react-router-dom": "^4.1.x"
  },
  "keywords": [
    "eazy",
    "easy-auth",
    "react",
    "redux",
    "authentication",
    "redux-saga",
    "react-auth",
    "react-router"
  ]
}
